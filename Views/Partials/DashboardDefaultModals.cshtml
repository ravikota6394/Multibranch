<!--Dashboard Amend Enquiry Modal screen - possibly moved to a new file later -->
<!--ng-controller="globalValues"-->
<div>

    <div class="modal fade" id="idDashboardAmendEnquiryChoice">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <form action="#" method="post" onsubmit="return false">
                    <div class="modal-header">
                        <a class="close" data-dismiss="modal">&times;</a>
                        <h4 class="modal-title">Amend Enquiry</h4>
                    </div>
                    <div class="modal-body no-padding">
                        <div class="well no-margin">
                            <div class="row">

                                <!-- Field: Enq code -->
                                <div class="col-xs-6 col-sm-3 form-group input-group-lg">
                                    <label>Reference</label>
                                    <input class="form-control" type="text" style="width: 250px;" ng-model="dashboardAECode" />
                                </div>
                            </div>


                        </div>
                    </div>
                    <div class="modal-footer">
                        <!--dashboardAESearch();-->
                        <a class="btn btn-success btn-sm" ng-click="dashboardAESearch();">Search</a>
                        <a class="btn btn-default btn-sm" data-dismiss="modal">Close</a>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- end of DashboardAmendEnquiryChoice -->
    <!-- Wrong Enquiry Code warning -->
    <div class="modal fade" id="idDashboardWrongEnquiryCodeWarning">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <form action="#" method="post">
                    <div class="modal-header">
                        <a class="close" data-dismiss="modal">&times;</a>
                        <h4 class="modal-title">Wrong Enquiry Reference</h4>
                    </div>
                    <div class="modal-body no-padding">
                        <div class="well no-margin">
                            <div class="row">

                                <h3>Invalid Enquiry Reference.</h3>
                            </div>


                        </div>
                    </div>
                    <div class="modal-footer">
                        <a class="btn btn-default btn-sm" data-dismiss="modal">close</a>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <!-- Ticker Tapes Modal -->
    <div class="modal fade" id="idDashboardTickerTaps" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title" style="text-align:center">Service Announcement</h4>
                </div>
                <div class="modal-body" style="height: 15em;">
                    <div class="container row col-sm-10 col-md-10 col-lg-10 col-sm-offset-1 col-md-offset-1 col-lg-offset-1">
                        <label>Service Announcement Text</label>
                        <p></p>
                        <textarea id="id-tickerText" class="form-control" type="text" style="width: 250px;" ng-model="tickerTape"></textarea>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="row">
                        <div class="col-xs-12" style="text-align:center">
                            <button class="btn btn-success btn-sm mb-10" type="button" ng-click="SaveTickerText()">
                                <i class="ace-icon fa fa-check bigger-110"></i>
                                Save
                            </button>
                            <button class="btn btn-danger btn-sm mb-10" type="reset" data-dismiss="modal">
                                <i class="ace-icon fa fa-times bigger-110"></i>
                                Cancel
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Duplicates Status Modal -->
    <div class="modal fade" id="idDuplicateStatus" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Duplicates Status</h4>
                </div>
                <div class="modal-body">
                    <p style="font-size: 16px;"><input type="checkbox" ng-model="duplicateStatus" id="chkDuplicatesStatus" name="duplicateStatus"> Set duplicate status</p>
                </div>
                <div class="modal-footer">
                    <div class="row">
                        <div class="col-xs-12" style="text-align:center">
                            <button class="btn btn-success btn-sm mb-10" type="button" ng-click="SaveDuplicatesStatus()">
                                <i class="ace-icon fa fa-check bigger-110"></i>
                                Save
                            </button>
                            <button class="btn btn-danger btn-sm mb-10" type="reset" data-dismiss="modal">
                                <i class="ace-icon fa fa-times bigger-110"></i>
                                Cancel
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- / Wrong Enquiry Code warning -->
