<!-- Client Operations Agreement Tab Content -->
<div class="row">
	<div class="col-xs-12">
		<div class="tab-content">
			<!-- Property Box -->
			<div class="widget-box transparent">
				<div class="widget-header">
					<h4 class="widget-title">
						<i class="ace-icon fa fa-home"></i>
						Property Details <small>Name of property here (editable example)</small>
					</h4>
					<div class="widget-toolbar">
						<a data-action="collapse" href="#">
							<i class="ace-icon fa fa-chevron-up"></i>
						</a>
					</div>
				</div>

				<div class="widget-body">
					<div class="widget-main">
						<div class="well well-sm">
							<form action="#" method="post">
								<div class="row">
									<!-- Field: Property # -->
									<div class="col-sm-4 col-xl-2">
										<div class="row form-group">
											<label class="col-lg-4 no-padding-right">Property #</label>
											<div class="col-lg-8">
												<input class="form-control" type="text" />
											</div>
										</div>
									</div>
									<!-- Field: Comission -->
									<div class="col-sm-4 col-xl-2">
										<div class="row form-group">
											<label class="col-lg-4 no-padding-right">Comission</label>
											<div class="col-lg-8">
												<select class="form-control">
													<option>Please select</option>
													<option>5%</option>
													<option>10%</option>
													<option>15%</option>
													<option>20%</option>
												</select>
											</div>
										</div>
									</div>
									<!-- Field: Tenancy -->
									<div class="col-sm-4 col-xl-2">
										<div class="row form-group">
											<label class="col-lg-4 no-padding-right">Tenancy</label>
											<div class="col-lg-8">
												<select class="form-control">
													<option>Please select</option>
													<option>Option #1</option>
													<option>Option #2</option>
													<option>Option #3</option>
												</select>
											</div>
										</div>
									</div>
									<!-- Field: Extension of Bookings -->
									<div class="col-sm-4 col-xl-3">
										<div class="row form-group">
											<label class="col-lg-5 no-padding-right" title="Extension of Bookings">Ext. of Bookings</label>
											<div class="col-lg-7">
												<select class="form-control">
													<option>Please select</option>
													<option>Option #1</option>
													<option>Option #2</option>
													<option>Option #3</option>
												</select>
											</div>
										</div>
									</div>
									<!-- Field: Deposits -->
									<div class="col-sm-4 col-xl-2">
										<div class="row form-group">
											<label class="col-lg-4 no-padding-right">Deposits</label>
											<div class="col-lg-8">
												<select class="form-control">
													<option>Please select</option>
													<option>Option #1</option>
													<option>Option #2</option>
													<option>Option #3</option>
												</select>
											</div>
										</div>
									</div>
								</div>

								<div class="row">
									<!-- Field: Payment Terms -->
									<div class="col-sm-4 col-xl-2 form-group">
										<label>Payment Terms</label>
										<textarea class="form-control"></textarea>
									</div>
									<!-- Field: Booking Procedure -->
									<div class="col-sm-4 col-xl-2 form-group">
										<label>Booking Procedure</label>
										<textarea class="form-control"></textarea>
									</div>
									<!-- Field: Charges -->
									<div class="col-sm-4 col-xl-2 form-group">
										<label>Charges <span data-content="Telephone Charges / Extra Charges" data-placement="top" data-rel="popover" data-trigger="hover" class="help-button popover-notitle">?</span></label>
										<textarea class="form-control"></textarea>
									</div>
									<!-- Field: Invoicing -->
									<div class="col-sm-4 col-xl-3 form-group">
										<label>
											Invoicing
											<span data-content="Invoicing / Charging / Application for Payment" data-placement="top" data-rel="popover" data-trigger="hover" class="help-button popover-notitle">?</span>
										</label>
										<textarea class="form-control"></textarea>
									</div>
									<!-- Field: Cancellation -->
									<div class="col-sm-4 col-xl-2 form-group">
										<label>Cancellation</label>
										<textarea class="form-control"></textarea>
									</div>
								</div>

								<div class="row">
									<div class="col-md-4">
										<label>Rates</label>
										<table class="table table-bordered table-striped bg-grey">
											<tr>
												<th>Room Type</th>
												<th>Rate per Night</th>
											</tr>
											<tr>
												<td>Studio</td>
												<td>$100</td>
											</tr>
											<tr>
												<td>Single</td>
												<td>$100</td>
											</tr>
											<tr>
												<td>Double</td>
												<td>$100</td>
											</tr>
											<tr>
												<td>Twin</td>
												<td>$100</td>
											</tr>
											<tr>
												<td>Triple</td>
												<td>$100</td>
											</tr>
										</table>
									</div>
									<div class="col-md-4 form-group">
										<label>Notes</label>
										<textarea class="form-control"></textarea>
									</div>
								</div>

								<div class="row">
									<div class="col-xs-12 text-right">
										<!-- Button state when editing is in progress -->
										<!-- <button class="btn btn-primary btn-sm disabled mb-10">
											<i class="ace-icon fa fa-pencil bigger-110"></i>
											Edit Property
										</button> -->
										<button class="btn btn-primary btn-sm mb-10">
											<i class="ace-icon fa fa-pencil bigger-110"></i>
											Edit Property
										</button>
										<button type="submit" class="btn btn-success btn-sm mb-10">
											<i class="ace-icon fa fa-check bigger-110"></i>
											Save Property
										</button>
										<button type="reset" class="btn btn-danger btn-sm mb-10">
											<i class="ace-icon fa fa-times bigger-110"></i>
											Cancel
										</button>
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
			<!-- End Property Box -->

			<div class="row">
				<div class="col-xs-12 text-center">
					<a href="#" class="btn btn-yellow mb-10">
						<i class="ace-icon fa fa-plus bigger-110"></i>
						Add another property
					</a>
				</div>
			</div>
		</div>
	</div>
</div><!-- End Client Operations Agreement Tab Content -->

