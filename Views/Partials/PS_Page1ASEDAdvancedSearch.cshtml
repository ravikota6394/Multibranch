                                                    </div>
                                                    <div class="col-sm-4 col-md-4 col-lg-4">
                                                        <button id="savePV_EnquiriesDashboard" class="btn btn-default btn-block mt-30" ng-click="SaveQueryName(CurrentQueryName,UserQueryId)" type="button">
                                                            <i class="fa fa-save bigger-110"></i>
                                                            Save Query
                                                        </button>
                                                    </div>
                                                </div>

                                                @*</form>*@
                                            </div>
                                        </div>
                                    </form>
                                </div>


                                <!--Modal for save query-->
                                <div class="modal fade" id="saveQuery" role="dialog">
                                    <div class="modal-dialog">

                                        <!-- Modal content-->
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <button type="button" class="close" ng-click="CancelQuery()">&times;</button>
                                                <h4 class="modal-title" style="text-align: center">Name the Query</h4>
                                            </div>
                                            <div class="modal-body" style="height: 20em;">
                                                <div class="container row col-sm-10 col-md-10 col-lg-10 col-sm-offset-1 col-md-offset-1 col-lg-offset-1">
                                                    <div class="form-inline">
                                                        <label class="col-sm-4">Query Name:</label>
                                                        <div class="col-sm-8">
                                                            <input ng-model="queryName" id="queryName" class="form-control" type="text" />
                                                            <label id="nameOfQuery" hidden style="color: red; display: none"></label><br /><br />
                                                        </div>
                                                    </div>
                                                    <div class="form-inline">
                                                        @if (Request.IsAuthenticated && ((User.IsInRole("Admin") || User.IsInRole("SuperAdmin") || User.IsInRole("Senior Manager") || User.IsInRole("Manager"))))
                                                        {
                                                            <div class="col-sm-4">
                                                                <input type="radio" id="isReportQuery" value="true" ng-model="reportQuery" class="queryType" name="privateQuery" ng-click="ShowReportQueries()"> <label>Report Query </label>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <input type="radio" id="isPrivateQuery" value="true" ng-model="privateQuery" class="queryType" name="privateQuery" ng-click="ShowReportQueries()"> <label>Private Query </label>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <input type="radio" id="isGlobalQuery" value="true" ng-model="globalQuery" class="queryType" name="privateQuery" ng-click="ShowReportQueries()"> <label>Global Query </label>
                                                            </div>
                                                            <input type="hidden" id="IsAdminCheck" value="true" />
                                                        }
                                                        else
                                                        {
                                                            <input type="hidden" id="IsAdminCheck" value="false" />
                                                        }
                                                    </div>
                                                    <div ng-show="hideReportQueries">
                                                        <div class="form-inline row no-margin-left">
                                                            <div class="col-sm-6">
                                                                <input type="radio" id="isInternalReport" value="true" ng-model="internalReport" class="reportQueryType" name="reportQueryType" ng-click="ShowSubReportQueries()"> <label>Internal Report </label>
                                                            </div>
                                                        </div>
                                                        <div ng-show="hideSubReportQueries">
                                                            <div class="form-inline row no-margin-left">
                                                                <div class="col-sm-1"></div>
                                                                <div class="col-sm-6">
                                                                    <input type="radio" id="isReservationsReport" value="true" ng-model="reservations" class="subReportQueryType" name="subReportQueryType"> <label>Reservations </label>
                                                                </div>
                                                            </div>
                                                            <div class="form-inline row no-margin-left">
                                                                <div class="col-sm-1"></div>
                                                                <div class="col-sm-6">
                                                                    <input type="radio" id="isSalesReport" value="true" ng-model="sales" class="subReportQueryType" name="subReportQueryType"> <label>Sales </label>
                                                                </div>
                                                            </div>
                                                            <div class="form-inline row no-margin-left">
                                                                <div class="col-sm-1"></div>
                                                                <div class="col-sm-6">
                                                                    <input type="radio" id="isSupportReport" value="true" ng-model="support" class="subReportQueryType" name="subReportQueryType"> <label>Supply </label>
                                                                </div>
                                                            </div>
                                                            <div class="form-inline row no-margin-left">
                                                                <div class="col-sm-1"></div>
                                                                <div class="col-sm-6">
                                                                    <input type="radio" id="isReportingReport" value="true" ng-model="reporting" class="subReportQueryType" name="subReportQueryType"> <label>Reporting </label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="form-inline row no-margin-left">
                                                            <div class="col-sm-6">
                                                                <input type="radio" id="isExternalReport" value="true" ng-model="externalReport" class="reportQueryType" name="reportQueryType" ng-click="ShowSubReportQueries()"> <label>External Report </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <br />
                                                    <label id="id_ValidateQueryType" hidden style="color: red; display: none"></label><br /><br />
                                                </div>
                                            </div>
                                            <br /><br />
                                            <div class="modal-footer">
                                                <div class="row">
                                                    <div class="col-xs-12" style="text-align: center">
                                                        <button class="btn btn-success btn-sm mb-10" type="button" ng-click="SaveQuery()">
                                                            <i class="ace-icon fa fa-check bigger-110"></i>
                                                            Save
                                                        </button>
                                                        <button class="btn btn-danger btn-sm mb-10" type="button" ng-click="CancelQuery()">
                                                            <i class="ace-icon fa fa-times bigger-110"></i>
                                                            Cancel
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--Ended Modal for save query-->

                                <!--Modal for update query-->
                                <div class="modal fade" id="update-Query" role="dialog">
                                    <div class="modal-dialog">

                                        <!-- Modal content-->
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                                <h4 class="modal-title">Update your Query</h4>
                                            </div>
                                            <div class="modal-body">
                                                <h6>Do you want to update your query?</h6>
                                                <div></div>
                                                <div class="form-inline">
                                                    <label>Rename Query:</label>
                                                    <input type="text" ng-model="renameQuery" id="renameQuery" class="form-control" />
                                                    <label id="newNameOfQuery" hidden style="color: red; display: none"></label><br /><br />
                                                </div>
                                            </div>
                                            <div class="modal-footer">
                                                <div class="row">
                                                    <div class="col-xs-12" style="text-align: center">
                                                        <button class="btn btn-success btn-sm mb-10" type="button" ng-click="UpdateQuery(UserQueryId,renameQuery)">
                                                            <i class="ace-icon fa fa-check bigger-110"></i>
                                                            Yes
                                                        </button>
                                                        <button class="btn btn-danger btn-sm mb-10" type="button" data-dismiss="modal">
                                                            <i class="ace-icon fa fa-times bigger-110"></i>
                                                            No
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <!--Ended Modal for update query-->

                            }
                            <style>
                                .datepicker {
                                    z-index: 1151 !important;
                                }

                                .control-label {
                                    text-align: right;
                                }
                            </style>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@*</div>*@