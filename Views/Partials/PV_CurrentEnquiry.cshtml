<!-- Enquiry Details -->
<div  class="col-md-2 col-lg-2">
    <div class="widget-box transparent collapsed">
        <div class="widget-header">
            <h5 class="widget-title">
                <i class="ace-icon fa fa-bars"></i>
                Current Enquiry
            </h5>

            <div class="widget-toolbar">
                <a data-action="collapse" >
                    <i class="ace-icon fa fa-chevron-down"></i>
                </a>
            </div>
        </div>
        <div class="widget-body">
            <div class="widget-main no-padding">
                <table class="table enquiry-details-table bg-info">
                    <tr>
                        <th>Enquiry #</th>
                        <td><i class="ace-icon fa fa-caret-right blue"></i>{{globalEnCode}}</td>
                    </tr>
                    <tr>
                        <th>Status</th>
                        <td><span class="{{globalEnEDProgressCSS}} label label-sm  arrowed-in-right pull-left ">{{globalEnEDProgressWord}}</span></td>
                    </tr> 
                    <tr>
                        <th>Created</th>
                        <td><i class="ace-icon fa fa-caret-right blue"></i> {{globalEnEDDateAdded | date: 'dd-MM-yyyy'}}</td>
                    </tr>
                    <tr>
                        <th>Assigned User</th>
                        <td><i class="ace-icon fa fa-caret-right blue"></i> {{globalEnEDAssignedUser}}</td>
                    </tr>
                    <tr>
                        <th>Contact</th>
                        <td><i class="ace-icon fa fa-caret-right blue"></i>{{globalEnPrimaryContactName}}</td>
                    </tr>

                    <tr>
                        <th>Location</th>
                        <td><i class="ace-icon fa fa-caret-right blue"></i>{{globalEnEDCity}} </td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
</div><!-- End Enquiry Details -->
