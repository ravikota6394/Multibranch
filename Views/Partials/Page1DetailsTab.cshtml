                        <button class="btn btn-danger btn-sm mb-10" id="DontSaveDetailsToFivewin" ng-click="dontSaveDetailsToFivewin()">
                            <i class="ace-icon fa fa-undo bigger-110"></i>
                            Cancel
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
</div>
