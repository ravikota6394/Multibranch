<!--Modal Design For Displaying Email Received-->
<div class="modal fade in" id="emailreceived" role="dialog">
    <div class="modal-dialog modal-lg">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <div class="table-responsive">
                    <table style="width:100%" class="EMailTable   well well-sm">
                        <tr>
                            <th colspan="2" class="tableheader"><center>ENQUIRY&nbsp;&nbsp;{{EmailDetails.eTrakEnquiryID}}&nbsp;-&nbsp;EMAIL&nbsp;&nbsp;RESPONSE</center></th>
                        </tr>
                        <tr>
                            <td class="Tablelabel">Display From:</td>
                            <td class="TableBody">{{TrackingRecord.DisplayFrom}}</td>
                        </tr>
                        <tr>
                            <td class="Tablelabel">Subject:</td>
                            <td class="TableBody">{{TrackingRecord.Subject}}</td>
                        </tr>
                        <tr>
                            <td class="Tablelabel">Display To:</td>
                            <td class="TableBody">{{TrackingRecord.SentTo}}</td>
                        </tr>
                        <tr>
                            <td class="Tablelabel">CC:</td>
                            <td class="TableBody">{{TrackingRecord.CC}}</td>
                        </tr>
                        <tr>
                            <td class="Tablelabel">BCC:</td>
                            <td class="TableBody">{{TrackingRecord.BCC}}</td>
                        </tr>
                        <tr>
                            <td class="Tablelabel">DateTimeReceived:</td>
                            <td class="TableBody">{{TrackingRecord.DateTimeReceived | date:'yyyy-MM-dd  HH:mm:ss'}}</td>
                        </tr>
                        <tr>
                            <td class="Tablelabel" colspan="2">
                                <p id="scroller" ng-bind-html=Body style="width:843px;"></p>
                            </td>
                        </tr>
                        <tr>
                            <td class="Tablelabel">Attachments:</td>
                            <td class="TableBody"><a ng-hide="hideAttachments" ng-click="ShowAttachments(TrackingRecord.ID)">Click here to find attachments</a></td>
                        </tr>
                    </table>
                    <div>
                        @*<a style="cursor:pointer;font-size:17px" ng-click="ReplyToEmail(TrackingRecord.DisplayFrom,TrackingRecord.Subject,Body)">Click here to reply to email</a>*@
                        <center>
                            <a style="cursor:pointer; font-size: 17px;padding-left:5em" title="Click here to REPLY the Email" ng-click="ReplyToEmail(TrackingRecord.DisplayFrom,TrackingRecord.Subject,Body,TrackingRecord.DisplayTo,'','',TrackingRecord.DateTimeReceived)"><i class="fa fa-mail-reply"> Reply</i></a>
                            <a style="cursor:pointer; font-size: 17px;padding-left:5em" title="Click here to REPLY ALL the Email" ng-click="ReplyToEmail(TrackingRecord.DisplayFrom,TrackingRecord.Subject,Body,TrackingRecord.DisplayTo,TrackingRecord.CC,TrackingRecord.BCC,TrackingRecord.DateTimeReceived)"><i class="fa fa-mail-reply-all"> Reply All</i></a>
                            <a style="cursor:pointer; font-size: 17px;padding-left:5em" title="Click here to FORWARD the Email" ng-click="ForwardEmail(TrackingRecord.DisplayFrom,TrackingRecord.Subject,Body,TrackingRecord.DateTimeReceived,TrackingRecord.DisplayTo)"><i class="fa fa-mail-forward"> Forward</i></a>
                        </center>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
