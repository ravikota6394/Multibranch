<!--Modal for turn down report-->
<div class="modal fade" id="id-turndownReport" data-backdrop="static" data-keyboard="false" role="dialog">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title text-center">Enquiry turn down report</h4>
            </div>
            <div class="modal-body">
                <div class="container">
                    <form class="form-horizontal textFloat">
                        <div class="form-group">
                            <label class="col-sm-4 control-label textFloat"><b>Property</b></label>
                            <div class="col-sm-6">
                                <label class="control-label"><b>Reason for NoAvailability/Availability</b></label>
                            </div>
                        </div>
                        <div class="form-group" ng-repeat="property in Properties">
                            <label class="col-sm-4 control-label textFloat">{{property.prName}}({{property.prPropertyCode}})</label>
                            <div class="col-sm-5 col-lg-6">
                                <select class="form-control" id="id-availabilityReasons{{$index}}" ng-options="reason.av_Reason for reason in AvailabilityReasons | orderBy : 'av_Reason'" ng-model="availabilityReason" ng-change="saveAvailabilityReason(property, availabilityReason)">
                                    <option value="">Please Select</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-4"></label>
                            <div class="col-sm-5 col-lg-6">
                                <div ng-show="validateReason" id="validate-Reason">Please select the reasons before saving the report</div>
                            </div>
                        </div>

                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-xs-12 text-center">
                        <button class="btn btn-success btn-sm mb-10" ng-click="saveReport()" type="button">
                            <i class="ace-icon fa fa-check bigger-110"></i>
                            Save Report
                        </button>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
<!--Ended modal for turn down report-->

