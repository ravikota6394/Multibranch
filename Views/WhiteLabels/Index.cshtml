@model IEnumerable<eTrak.Dal.Models.WhiteLabel>

@{
    ViewBag.Title = "Index";
}

<h2>WhiteLabels</h2>

<p>
    <a href="/WhiteLabels/Create/" class="btn btn-success btn-sm">Create New</a>
    
</p>
<div ng-controller="R_WhiteLabels" class="container">
    <table id="idASPWhiteLabels" show-filter="true" dt-options="dtOptions" datatable="ng" ng-model="WhiteLabels" class="responsive   table table-striped table-bordered">
        <!--added extra datatable in class to see what happens-->
        <thead class="thin-border-bottom">
            <tr>
                <th data-class="expand">Name</th>
                <th data-class="expand">Language</th>
                <th data-class="expand">URL</th>
                <th data-hide="screen_xs,screen_sm">Status (L=Live, D=Deleted, I=Ignore) </th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>

            <tr ng-repeat="edRec in WhiteLabels ">
                <td>{{edRec.wlName}}</td>
                <td>{{edRec.lcName}}</td>
                <td>{{edRec.wlURL}}</td>
                <td>{{edRec.wlStatus}}</td>
                <td>
                    <a href="WhiteLabels/Edit/{{edRec.wlCode}}">Edit</a> |
                    <a href="WhiteLabels/Details/{{edRec.wlCode}}">Details</a>
                </td>
            </tr>

        </tbody>
    </table>
</div>
