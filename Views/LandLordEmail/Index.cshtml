                                    <option value="">Please Select</option>
                                    <option>Yes</option>
                                    <option>No</option>
                                </select>
                                <label id="taxApplied" hidden style="color: red; display: none"></label>
                            </p>
                            @*<p class="field-wrap">
                                <label for="aof-tomsTax">TOMS tax</label>
                                <select id="aof-tomsTax" name="aof-tomsTax" ng-model="prTomsTaxIncluded">
                                    <option value="">Please Select</option>
                                    <option>Yes</option>
                                    <option>No</option>
                                </select>
                            </p>*@
                            <p class="field-wrap">
                                <label for="aof-taxRate" class="label-required">Tax Rate Applicable<i class="fa fa-question-circle tooltip textcolor-primary" title="Tell us please, so we can contact you if necessary"></i></label>
                                <input id="aof-taxRate" placeholder="Please enter" name="aof-taxRate" ng-model="prTaxRate" class="required" type="text" ng-keypress="ValidateTaxRate($event)" />
                                <label id="taxRate" hidden style="color: red; display: none"></label>
                                <label id="validate_TaxRate" hidden style="color: red; display: none"></label>
                            </p>
