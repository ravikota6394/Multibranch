<body ng-controller="LandLordEmailController">

    <!--Modal for checking availability-->
    <div class="modal fade" id="id_IsAvailabile" data-backdrop="static" data-keyboard="false" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" style="text-align:center">Check Availability Status</h4>
                </div>
                <div class="modal-body" style="height: 10em;">
                    <div class="col-sm-offset-3">
                        <br />
                        <div style="font-size: 17px;">
                            <input type="radio" id="isAvailable" value="true" name="availablility" ng-model="isAvailable" ng-click="DissmissAvailabilityCheck();"> <span>Availability </span>
                        </div>
                        <br />
                        <div style="font-size: 17px;">
                            <input type="radio" id="isNotAvailable" value="true" name="noavailability" ng-model="isNotAvailable" ng-click="ConfirmNoAvailability();"> <span>No Availability </span>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="row"></div>
                </div>
            </div>
        </div>
    </div>
    <!--Ended Modal for checking availability-->
    <!--Modal for confirming noavailability-->
    <div class="modal fade" id="id_ConfirmAvailability" data-backdrop="static" data-keyboard="false" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" style="text-align:center"></h4>
                </div>
                <div class="modal-body">
                    <h6 style="text-align:center">You are about to confirm 'No Availability' for this Enquiry. Click ok to proceed.</h6>
                </div>
                <div class="modal-footer">
                    <div>
                        <button type="button" class="btn btn-success" ng-click="NoAvailabity()">OK</button>
                        <button type="button" class="btn btn-danger" ng-click="DismissNoAvailability()">Cancel</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--Ended Modal for confirming noavailability-->
    <!--Modal for checking whether the apartment type changed or not-->
    <div class="modal fade" id="id_ChangeSuppliedApartmentType" data-backdrop="static" data-keyboard="false" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body">
                    <div><h6 style="line-height: 35px;text-align: center;">Please note the client has requested the Apartment Type as advised. If you change this, please tell us why in the Accommodation Offer Note. Do you want to change the Apartment Type?</h6></div>
                </div>
                <div class="modal-footer">
                    <div>
                        <button type="button" class="btn btn-success" ng-click="AgreeToChangeApartmentType()">Yes</button>
                        <button type="button" class="btn btn-danger" ng-click="NotToChangeApartmentType()">No</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--Ended Modal for checking whether the apartment type changed or not-->
    <header class="page-header" ng-show="InputDivVisible">
        <div class="row">
            <input type="hidden" id="txtEmailKey" value="@ViewBag.EmailKey">
            <div class="small-3 medium-6 columns">
                <a href="https://v2.apartmentservice.com" target="_blank">
                    <img src="https://forms.apartmentservice.com/etrak/images/newsletter/logo-header.png" alt="The Apartment Service" style="display: block;" height="61" width="112">
                </a>
            </div>
            <div class="small-9 medium-6 columns">
                <ul class="contact-list list-unstyled">
                    <li>Reservations:&nbsp;<span>+44 (0) 20 8944 1444</span></li>
                    <li>Corporate Sales:&nbsp;<span>+44 (0) 20 8944 3612</span></li>
                </ul>
            </div>
        </div>
    </header>
    <!-- ===== Page Content ===== -->
    <div class="page-content" ng-show="InputDivVisible">
        <div class="row">
            <div class="small-12 medium-8 columns">
