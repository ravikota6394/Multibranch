                                    <option value="">Please Select</option>
                                    <option ng-repeat="apartment in ApartmentTypes | orderBy : 'Name'" value="{{apartment.Name}}">{{apartment.Name}}</option>
                                    <option value="Other">Other</option>
                                </select> <br />
                                <input type="text" id="apartment-Type" ng-model="atDescriptionWhenOther" ng-blur="ChangeApartmentText(atDescriptionWhenOther)" style="display: none" />
